{
  "version": 2,
  "name": "tanda-2019-presentation-",
  "builds": [
    { "src": "static/**", "use": "@now/static" },
    {
      "src": "api/**",
      "use": "@now/node",
      "config": { "maxLambdaSize": "50mb" }
    },
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "public": false,
  "alias": ["tanda.now.sh"],
  "github": {
    "autoAlias": true
  },
  "regions": ["all"],
  "routes": [{ "src": "/api", "dest": "/api/index.js" }]
}
